{
  "name": "ecommerce-backend",
  "version": "1.0.0",
  "description": "E-commerce backend API with product comparison features and admin dashboard support",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "seed": "node dist/scripts/createSampleData.js",
    "create-admin": "node dist/scripts/recreateAdmin.js",
    "db:migrate": "sequelize db:migrate",
    "db:seed:all": "sequelize db:seed:all",
    "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed:all"
  },
  "keywords": [
    "ecommerce",
    "api",
    "nodejs",
    "express",
    "sequelize",
    "admin-dashboard",
    "product-management"
  ],
  "author": "WallStreetCS",
  "license": "MIT",
  "dependencies": {
    "@types/nodemailer": "^6.4.17",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.56.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mssql": "^11.0.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.1",
    "puppeteer": "^24.13.0",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7",
    "tedious": "^18.6.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.19.1",
    "@types/sequelize": "^4.28.20",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.1",
    "ts-jest": "^29.4.0"
  }
}
